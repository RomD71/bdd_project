<html>
<body>
<h1>Liste des article !!!</h1><br/><br/>


{{ form(formArticle) }}<br/>

<table border="1">
    <tr>
        <th>ID Article</th>
        <th>Titre</th>
        <th>Contenu</th>
        <th>Date de publication</th>
    </tr>
    {% for item in article %}
        <tr>
            <td>{{ item.idArticle }}</td>
            <td>{{ item.titre }}</td>
            <td>{{ item.contenuArticle }}</td>
            <td>{{ item.datePublication|date("d/m/Y") }}</td>
        </tr>
    {% endfor %}
</table>
</body>
</html>